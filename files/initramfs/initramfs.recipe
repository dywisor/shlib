SET_DEFAULTS
SET_BUSYBOX_ASH
[ "${TARGET_SHLIB_ROOT}" != "/sh" ] || TARGET_ROOT /
use large || SHLIBCC_ARGS_APPEND --strip-all --no-enclose-modules

INTO /${D:-initramfs}

ENABLE_OVERWRITE_IF lib/init-functions.sh init functions.sh telinit

SPLITLIB    initramfs/default init-functions.sh
LINK_SHARED initramfs/default init init-functions.sh

SYMSTORM  lib/init-functions.sh functions.sh

COPY_SCRIPT initramfs/telinit telinit
